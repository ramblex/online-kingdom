<div id="left-col">
  <div class="box">
    <div class="box-title">
      <span class="blue">AWARDS</span> PAGE<br />
      <span class="subtitle">CONTENT <span>CHANGER</span></span>
    </div>
    <ul id="awards-switcher" class="switcher">
      <%= render :partial => 'shared/switcher_list' %>
    </ul>

    <div id="awardlist">
      <%= render :partial => @awards %>
    </div>
  </div>
</div>

<div id="right-col">
  <%= render :partial => 'shared/official_partners' %>
</div>

<% content_for :extra_js do %>
  <%= javascript_include_tag 'switcher' %>
  <script type="text/javascript">
    switcher('#awards-switcher', '#awardlist');
  </script>
<% end %>
