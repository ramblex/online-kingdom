<div id="wide-left-col">
  <div class="box">
    <div class="box-title">
      <span class="blue">INSIDER</span> PAGE<br />
    </div>
    <div class="matches-title">ONLINE KINGDOM INSIDER PLAN - WHAT IS IT?</div>
    <%= @insider.description %>
    <div class="matches-title">FREQUENTLY ASKED QUESTIONS</div>
    <%= @insider.faq %>
  </div>
</div>

<div id="thin-right-col">
  <div class="box">
    <div class="box-title">
      <span class="blue">MONTH</span> PRIZES<br />
    </div>
    <ul class="insider">
      <%= render :partial => @insider.insider_prizes %>
    </ul>
    <div class="clear"></div>
  </div>

  <div class="box">
    <div class="box-title">
      <span class="blue">MONTH</span> EVENTS<br />
    </div>
    <ul class="insider">
      <%= render :partial => @events %>
    </ul>
    <div class="clear"></div>
  </div>

  <div class="box">
    <div class="box-title">
      <span class="blue">FILE</span> VAULT<br />
    </div>
    <ul class="insider">
      <%= render :partial => @files %>
    </ul>
    <div class="clear"></div>
  </div>
</div>
